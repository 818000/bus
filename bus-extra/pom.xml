        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <lombok.version>1.18.38</lombok.version>
        <fastjson.version>2.0.57</fastjson.version>
        <gson.version>2.13.1</gson.version>
        <jackson.version>2.19.0</jackson.version>
        <jpinyin.version>1.1.8</jpinyin.version>
        <pinyin4j.version>2.5.1</pinyin4j.version>
        <tiny.version>2.0.3.RELEASE</tiny.version>
        <bopomofo4j.version>1.0.1</bopomofo4j.version>
        <houbb.version>0.4.0</houbb.version>
        <jsch.version>0.2.25</jsch.version>
        <sshj.version>0.39.0</sshj.version>
        <zxing.version>3.5.3</zxing.version>
        <net.version>3.11.1</net.version>
        <emoji.java.version>5.1.1</emoji.java.version>
        <compress.version>1.27.1</compress.version>
        <jakarta.mail.version>2.1.3</jakarta.mail.version>
